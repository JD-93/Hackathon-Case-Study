#include <sstream>
#include <vector>
#include <algorithm>
#include <iomanip>
#include <iostream>
#include <fstream>
using namespace std;


class emp
{
public :
	string emp_no,name,sal,deptno ;
	void display()
	{
		cout<<emp_no<<","<<name<<","<<sal<<","<<deptno<<endl ;
	}
};
class dept
{
public :
	string dept_no,name,city ;
	vector<emp>  emps ;
	void display()
	{
		cout<<dept_no<<","<<name<<","<<city<<endl ;
		cout<<" \t- " ;
		for (size_t i=0 ; i<this->emps.size() ; i++ ){
			emps[i].display() ;
		}
	}
};


int main() {








}

