#include "preferences.h"

preferences::preferences() {
	// TODO Auto-generated constructor stub

}const string& preferences::getCenterId() const {
	return center_id;
}

void preferences::setCenterId(const string &centerId) {
	center_id = centerId;
}

const string& preferences::getCourseName() const {
	return course_name;
}

void preferences::setCourseName(const string &courseName) {
	course_name = courseName;
}

const string& preferences::getFormNo() const {
	return form_no;
}

void preferences::setFormNo(const string &formNo) {
	form_no = formNo;
}

const string& preferences::getPrefNo() const {
	return pref_no;
}

void preferences::setPrefNo(const string &prefNo) {
	pref_no = prefNo;
}

preferences::~preferences() {
	// TODO Auto-generated destructor stub
}


void preferences::display() {
	cout<<"form no : "<<this->getFormNo()<<endl ;
	cout<<"pref no :  : "<<this->getPrefNo()<<endl ;
	cout<<"course name : "<<this->getCourseName()<<endl ;
	cout<<"center id : "<<this->getCenterId()<<endl ;
	cout<<"\n"<<endl ;
}
